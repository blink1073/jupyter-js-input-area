{
  "name": "input-area",
  "version": "0.0.2",
  "description": "Jupyter Input Area",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "codemirror": "^5.7.0",
    "phosphor-messaging": "^1.0.5",
    "phosphor-properties": "^2.0.0",
    "phosphor-signaling": "^1.1.2",
    "phosphor-widget": "^1.0.0-beta"
  },
  "devDependencies": {
    "nodemon": "^1.8.0",
    "rimraf": "^2.4.3",
    "steal": "^0.12.3",
    "typedoc": "^0.3.12",
    "typescript": "^1.6.2"
  },
  "scripts": {
    "clean": "npm run clean:lib && npm run clean:example",
    "clean:lib": "rimraf lib",
    "clean:example": "rimraf example/*.js example/*.map",
    "build:src": "tsc --project src",
    "build:test": "tsc --project test/src",
    "build:example": "tsc --project example",
    "build": "npm run build:src",
    "docs": "rimraf docs/api && typedoc --options scripts/tdoptions.json",
    "postinstall": "npm dedupe",
    "watch": "npm run watch:build -s",
    "watch:build": "nodemon -q -w src/ --ext ts,js --exec 'npm run build'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jupyter/jupyter-js-input-area.git"
  },
  "files": [
    "lib/index.js",
    "lib/index.css",
    "lib/index.d.ts"
  ],
  "system": {
    "npmIgnore": [
      "devDependencies"
    ]
  },
  "keywords": [
    "jupyter",
    "input",
    "input-area"
  ],
  "author": "Jupyter Project",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jupyter/jupyter-js-input-area/issues"
  },
  "homepage": "https://github.com/jupyter/jupyter-js-input-area#readme"

}
